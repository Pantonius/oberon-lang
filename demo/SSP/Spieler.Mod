MODULE Spieler;CONST    Schere* = 0;    Stein* = 1;    Papier* = 2;TYPE    Spieler* = POINTER TO SpielerDesc;    SpielerDesc* = RECORD        name* : ARRAY 32 OF CHAR    END;    Symbol* = SHORTINT;(* abstrakte Schnittstelle f√ºr Spieler *)PROCEDURE (sp : Spieler) InitSpieler* (S : INTEGER);(* Initialisiert den Spieler. Am Turnier nehmen S (S < MaxSpieler) verschiedene Spieler teil *)END InitSpieler;PROCEDURE (sp : Spieler) ZeigeSymbol* (p : INTEGER) : Symbol;(* Diese Prozedur befragt Ihren Spieler, welches Symbol er dem Spielerpartner p zeigen will *) END ZeigeSymbol;PROCEDURE (sp : Spieler) PartnerZeigte* (p : INTEGER; s : Symbol);(* Was war die Entscheidung des Partners p beim letzten Spiel? Welches Symbol hat er gezeigt? *)END PartnerZeigte;END Spieler.